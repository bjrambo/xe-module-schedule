<query id="updateUploadedCount" action="update">
	<tables>
		<table name="schedules" />
	</tables>
	<columns>
		<column name="uploaded_count" var="uploaded_count" default="0" filter="number" />
	</columns>
	<conditions>
		<condition operation="equal" column="schedule_srl" var="schedule_srl" filter="number" notnull="notnull" />
	</conditions>
</query>