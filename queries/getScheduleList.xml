<query id="getScheduleList" action="select">
	<tables>
		<table name="schedule_list" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="selected_date" var="selected_date" />
		<condition operation="equal" column="module_srl" var="module_srl" pipe="and" />
	</conditions> 
</query>
