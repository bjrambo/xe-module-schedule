<query id="updateScheduleStatus" action="update">
    <tables>
        <table name="schedules" />
    </tables>
    <columns>
        <column name="status" var="status" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="schedule_srl" var="schedule_srl" filter="number" notnull="notnull" />
    </conditions>
</query>