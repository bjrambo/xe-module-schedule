<filter name="input_password" module="schedule" act="procScheduleVerificationPassword">
	<form>
		<node target="schedule_srl" required="true" />
		<node target="password" required="true" />
	</form>
	<parameter>
		<param name="mid" target="mid" />
		<param name="schedule_srl" target="schedule_srl" />
		<param name="password" target="password" />
	</parameter>
	<response>
		<tag name="error" />
		<tag name="message" />
	</response>
</filter>